<!DOCTYPE html>
<html>
<head>
<title>Dice Combinations Sum</title>
<script>

var sum=231;
var counter=0;

for (i=1;i<=6;i=i+1)

{
    for (j=1;j<=6;j=j+1)
    
    {
    document.write(i+" , "+j+"<br>");
    }
    
    {
    sum=sum+i
    counter=counter+1
    }

if (counter==6){break;}

}

document.write("<br>"+sum)

</script>

</html>
</head>
