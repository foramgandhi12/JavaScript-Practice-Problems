<!DOCTYPE html>
<html>
<head>
<title>Grade Calculator</title>
<script type='text/javascript'>

var grade=74

if (grade<50)
    {
    document.write("F")
    }

else if ((grade==50) || (grade<60))
    {
    document.write("E")
    }

else if ((grade==60) || (grade<70))
    {
    document.write("D")
    }

else if ((grade==70) || (grade<80))
    {
    document.write("C")
    }

else if ((grade==80) || (grade<90))
    {
    document.write("B")
    }

else if (grade>=90)
    {
    document.write("A")
    }

</script>
</head>
<body>

</body>
</html>
