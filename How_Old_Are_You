<!DOCTYPE html>
<html>
<head>
<title>How Old Are You?</title>
<script type='text/javascript'>

var i=prompt("How old are you?",25);
    document.write(i)

if (i<=2)
{
    alert("You are an infant.");
}

else if (i<=9 )
{
    alert("You are a kid.");
}

else if (i<=19)
{
    alert("You are a teenager");
}

else if (i<=150)
{
    alert("You are an adult");
}

else if (150<i)
{
    alert("You are supposed to be dead.");
}

</script>
</head>
<body>

</body>
</html>
