<!DOCTYPE html>
<html>
<head>
<title>Type your Page Title here.</title>
<script type='text/javascript'>

var x=10
var y=11

if (x>y)

    {
    document.write(x+" is greater than "+y+".");
    }

else if (x<y)

    {
    document.write(x+" is less than "+y+".");
    }

else if (x=y)

    {
    document.write(x+" is equal to "+y+".");
    }

if (x%2==0)

    {
    document.write("<br>" +x+" is an even number"+".")
    }

else if (x%2==1)

    {
    document.write ("<br>" + x+" is an odd number"+".")
    }

if (y%2==0)

    {
    document.write("<br>" + y+" is an even number"+".")
    }

else if (y%2==1)

    {
    document.write ("<br>" + y+" is an odd number"+".")
    }

</script>
